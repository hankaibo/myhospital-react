{
  "name": "myhospital",
  "version": "1.0.0",
  "private": true,
  "description": "北京定点医疗机构选择助手",
  "homepage": "http://47.95.120.23:9002",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "prettier": "prettier -c --write \"src/**/*\"",
    "postinstall": "umi g tmp & husky install",
    "start": "cross-env UMI_ENV=dev umi dev",
    "dev": "npm run start:dev",
    "pre": "npm run start:pre",
    "start:dev": "cross-env PORT=9002 REACT_APP_ENV=dev MOCK=none UMI_ENV=dev UMI_UI=none umi dev",
    "start:pre": "cross-env PORT=9002 REACT_APP_ENV=pre MOCK=none UMI_ENV=dev UMI_UI=none umi dev"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "eslint --ext .js,.jsx",
    "**/*.{js,jsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@ant-design/pro-layout": "^6.25.4",
    "antd": "^4.16.13",
    "antd-img-crop": "^3.16.0",
    "classnames": "^2.3.1",
    "lodash.debounce": "^4.0.8",
    "lodash.foreach": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.1",
    "ol": "^6.7.0",
    "ol-contextmenu": "^4.1.0",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.0",
    "react": "^16.14.0",
    "react-dev-inspector": "^1.6.0",
    "react-dom": "^16.14.0",
    "react-helmet-async": "^1.1.2",
    "sockjs-client": "^1.5.2",
    "stompjs": "^2.3.3",
    "tinycolor2": "^1.4.2",
    "umi": "^3.5.18",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@umijs/plugin-esbuild": "^1.1.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.24",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.4.0",
    "stylelint": "^13.12.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^4.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
